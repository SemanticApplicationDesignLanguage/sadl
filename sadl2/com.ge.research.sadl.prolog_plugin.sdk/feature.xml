<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.ge.research.sadl.prolog_plugin.sdk"
      label="tuProlog-Plugin for SADL feature"
      version="2.4.0.qualifier"
      provider-name="GE Global Research">

   <description url="http://www.example.com/description">
      The SADL tuProlog Plug-in is a tuProlog-based reasoner for the Semantic
Application Design Language (SADL), which is a controlled
English grammar and Eclipse IDE for building, testing, and deploying
semantic models.
   </description>

   <copyright url="http://www.example.com/copyright">
      Copyright General Electric, 2015.
   </copyright>

   <license url="http://sadl.sourceforge.net">
      The SADL Prolog Plug-in is available under the Eclipse Public License.

The plug-in requires that TuProlog be downloaded and installed separately.
   </license>

   <url>
      <discovery label="tuProlog update site" url="http://tuprolog.googlecode.com/svn/2p-plugin/trunk/tuPrologUpdateSite/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="com.ge.research.sadl" version="2.2.0" match="greaterOrEqual"/>
      <import plugin="com.ge.research.sadl.jena-plugin" version="2.3.0" match="greaterOrEqual"/>
      <import plugin="tuPrologPlugin" version="2.9.1" match="greaterOrEqual"/>
      <import plugin="org.apache.log4j"/>
   </requires>

   <plugin
         id="com.ge.research.sadl.prolog.plugin"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
