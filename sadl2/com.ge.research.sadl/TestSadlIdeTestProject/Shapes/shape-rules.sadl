import "file://shapes-specific.sadl" as shapes-specific.

Rule AreaOfCircle
	given
		x is any Circle
	if
		
	then
		area of x = radius of x^2 * 3.14159 .
		
Rule AreaOfRectangle
	given
		x is any Rectangle
	if
		
	then
		area of x = height of x * width of x .
