{
"name": "Domainmodel",
"scopeName": "text.dmodel",
"fileTypes": [
    "dmodel"
],
"repository": {
    "general": {
        "patterns": [
            {
                "include": "#linecomment"
            },
            {
                "include": "#blockcomment"
            },
            {
                "include": "#keyword"
            }
        ]
    },
    "linecomment": {
        "name": "comment.line.double-dash.dmodel",
        "begin": "(^[ \\t]+)?(?=//)",
        "end": "(?=$)"
    },
    "blockcomment": {
        "name": "comment.block.dmodel",
        "begin": "/\\*(\\*)?(?!/)",
        "end": "\\*/"
    },
    "keyword": {
        "name": "keyword.control.mydsl",
        "match": "\\b(entity|datatype)\\b|!"
 
    }
},
"patterns": [
    {
        "include": "#general"
    }
],
