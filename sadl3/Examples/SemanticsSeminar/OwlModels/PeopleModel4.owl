<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:sadlbasemodel="http://sadl.org/sadlbasemodel"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:sadlimplicitmodel="http://sadl.org/sadlimplicitmodel"
    xmlns:builtinfunctions="http://sadl.org/builtinfunctions"
    xmlns:j.0="http://sadl.org/sadlimplicitmodel#"
    xmlns:PeopleModel4="http://sadl.org/PeopleModel4.sadl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="http://sadl.org/PeopleModel4.sadl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="builtinfunctions"/>
    <owl:imports rdf:resource="sadlimplicitmodel"/>
    <owl:imports rdf:resource="sadlbasemodel"/>
    <rdfs:comment xml:lang="en">This ontology was created from a SADL file 'PeopleModel4.sadl' and should not be directly edited.</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:ID="Man">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Person"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Marriage"/>
  <owl:Class rdf:ID="Woman">
    <rdfs:subClassOf rdf:resource="#Person"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="brother">
    <rdfs:range rdf:resource="#Man"/>
    <rdfs:domain rdf:resource="#Person"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="sister">
    <rdfs:range rdf:resource="#Woman"/>
    <rdfs:domain rdf:resource="#Person"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="child">
    <rdfs:range rdf:resource="#Person"/>
    <rdfs:domain rdf:resource="#Person"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="husband">
    <rdfs:domain rdf:resource="#Marriage"/>
    <rdfs:range rdf:resource="#Man"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="son">
    <rdfs:subPropertyOf rdf:resource="#child"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="wife">
    <rdfs:domain rdf:resource="#Marriage"/>
    <rdfs:range rdf:resource="#Woman"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="favoriteThing">
    <rdfs:domain rdf:resource="#Person"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="location">
    <rdfs:domain rdf:resource="#Marriage"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="age">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#Person"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="date">
    <rdfs:domain rdf:resource="#Marriage"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
  </owl:DatatypeProperty>
  <owl:SymmetricProperty rdf:ID="sibling">
    <rdfs:range rdf:resource="#Person"/>
    <rdfs:domain rdf:resource="#Person"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:SymmetricProperty>
  <j.0:NamedQuery rdf:ID="FindPeopleByAgeRange">
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >select ?p where {?p &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#type&gt; &lt;http://sadl.org/PeopleModel4.sadl#Person&gt; . ?p &lt;http://sadl.org/PeopleModel4.sadl#age&gt; ?ag . FILTER(?ag &gt;= ? &amp;&amp; ?ag &lt;= ? )}</rdfs:isDefinedBy>
  </j.0:NamedQuery>
  <j.0:Rule rdf:ID="siblingRule"/>
  <j.0:NamedQuery rdf:ID="FindAdults">
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >select ?p where {?p &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#type&gt; &lt;http://sadl.org/PeopleModel4.sadl#Person&gt; . ?p &lt;http://sadl.org/PeopleModel4.sadl#age&gt; ?ag . FILTER (?ag &gt;= 18)} order by ?ag</rdfs:isDefinedBy>
  </j.0:NamedQuery>
</rdf:RDF>
